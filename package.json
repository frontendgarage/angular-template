{
  "name": "angular-template",
  "version": "7.7.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:unit && npm run test:integration",
    "test:ci": "npm run test:unit:ci && npm run test:integration:ci",
    "test:unit": "ng test -- --coverage",
    "test:unit:ci": "ng test -- --ci",
    "test:integration": "jest --config=jest.config.integration.js",
    "test:integration:ci": "jest --config=jest.config.integration.js --ci",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/router": "12.2.7",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.2",
    "@angular-devkit/build-angular": "12.2.7",
    "@angular/cli": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@types/jest": "27.0.2",
    "@types/node": "12.20.26",
    "jest": "27.2.1",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "simple-git-hooks": "2.6.1",
    "typescript": "4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  }
}
